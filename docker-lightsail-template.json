{
    "containers": {
        "app": {
            "image": "",
            "environment": {
                "App_ENV": "release",
                "NODE_ENV": "production"
            },
            "ports": {
                "3002": "HTTP"
            }
        }
    },
    "publicEndpoint": {
        "containerName": "app",
        "containerPort": 3000,
        "healthCheck": {
            "healthThreshold": 2,
            "unhealthThreshold": 2,
            "timeoutseconds": 15,
            "intervalSeconds": 20,
            "path": "/",
            "successCodes": "200-499"
        }
    }
}